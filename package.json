{
  "name": "openscad-babylon",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:run": "vitest run",
    "test:e2e": "npx vitest run src/babylon-csg2/e2e-tests --reporter=verbose",
    "validate": "npx tsx scripts/validate-complete-pipeline.ts",
    "validate:pipeline": "npx vitest run src/babylon-csg2/openscad-pipeline --reporter=verbose",
    "validate:visitor": "npx vitest run src/babylon-csg2/openscad-ast-visitor --reporter=verbose",
    "typecheck": "npx tsc --noEmit",
    "test:glob": "glob \"src/**/*.test.{ts,tsx}\"",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "dependencies": {
    "@babylonjs/core": "^8.11.0",
    "@babylonjs/loaders": "^8.11.0",
    "@babylonjs/materials": "^8.11.0",
    "@holistic-stack/openscad-parser": "0.1.2",
    "@holistic-stack/tree-sitter-openscad": "0.1.0",
    "@playwright/experimental-ct-react": "^1.52.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-babylonjs": "latest",
    "web-tree-sitter": "0.25.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "~9.8.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "glob": "^11.0.2",
    "jsdom": "^24.1.1",
    "prettier": "^3.3.3",
    "shx": "^0.3.4",
    "typescript": "^5.8.3",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^1.6.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.10.0"
}
