{
  "name": "openscad-babylon",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc --project tsconfig.lib.json && vite build",
    "test": "node --max-old-space-size=8192 ./node_modules/vitest/vitest.mjs run",
    "test:memory-safe": "node --max-old-space-size=8192 ./node_modules/vitest/vitest.mjs run --fileParallelism=false",
    "validate": "npx tsx scripts/validate-complete-pipeline.ts",
    "validate:pipeline": "vitest run src/features/openscad-pipeline --reporter=verbose",
    "typecheck": "npx tsc --noEmit",
    "test:glob": "glob \"src/**/*.test.{ts,tsx}\"",
    "test:coverage": "vitest run --coverage --pool=forks --maxConcurrency=1",
    "test:ui": "vitest --ui",
    "test:code-editor": "echo 'Running code-editor tests individually to avoid isolation issues...' && vitest run src/features/code-editor/components/monaco-editor-simple.test.tsx --reporter=verbose && vitest run src/features/code-editor/components/store-connected-editor.test.tsx --reporter=verbose",
    "test:visual": "playwright test --config=playwright-ct.config.ts --reporter=line",
    "test:visual:ui": "playwright test --config=playwright-ct.config.ts --ui",
    "test:visual:debug": "playwright test --config=playwright-ct.config.ts --debug",
    "test:visual:update": "playwright test --config=playwright-ct.config.ts --update-snapshots --reporter=line",
    "biome:fix": "biome check --write .",
    "biome:lint": "biome lint .",
    "biome:format": "biome format --write .",
    "type-check": "tsc --noEmit",
    "test:ct": "playwright test --config=playwright-ct.config.ts",
    "test:ct:ui": "playwright test --config=playwright-ct.config.ts --ui",
    "test:ct:headed": "playwright test --config=playwright-ct.config.ts --headed",
    "test:ct:debug": "playwright test --config=playwright-ct.config.ts --debug",
    "test:ct:update-snapshots": "playwright test --config=playwright-ct.config.ts --update-snapshots --reporter=line",
    "test-ct": "playwright test -c playwright-ct.config.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:unit": "vitest run",
    "test:integration": "vitest run",
    "test:visual": "playwright test --config playwright-ct.config.ts",
    "test:watch": "vitest",
    "test:watch:integration": "vitest",
    "validate:types": "tsc --noEmit --strict",
    "validate:quality": "npm run lint && npm run validate:types && npm run validate:shared-utilities",
    "validate:all": "npm run validate:quality && npm run test:unit && npm run test:coverage",
    "fix:auto": "tsx scripts/ollama-lint-fixer.ts",
    "fix:auto:codegemma": "tsx scripts/ollama-lint-fixer.ts --model=codegemma:7b",
    "fix:auto:phi3": "tsx scripts/ollama-lint-fixer.ts --model=phi3:3.8b"
  },
  "dependencies": {
    "@babylonjs/core": "^8.16.1",
    "@babylonjs/gui": "^8.17.2",
    "@babylonjs/loaders": "^8.16.1",
    "@babylonjs/materials": "^8.16.1",
    "@monaco-editor/react": "4.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "earcut": "^3.0.2",
    "immer": "^10.1.1",
    "lodash.debounce": "^4.0.8",
    "monaco-editor": "0.52.2",
    "ollama": "^0.5.16",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-error-boundary": "^6.0.0",
    "reselect": "^5.1.1",
    "tslog": "^4.9.3",
    "web-tree-sitter": "0.25.3",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babylonjs/inspector": "^8.16.1",
    "@biomejs/biome": "2.0.6",
    "@eslint/js": "^9.8.0",
    "@playwright/experimental-ct-react": "^1.53.0",
    "@playwright/test": "^1.53.0",
    "@storybook/addon-a11y": "^9.0.12",
    "@storybook/addon-docs": "9.0.12",
    "@storybook/addon-onboarding": "9.0.12",
    "@storybook/react": "^9.0.12",
    "@storybook/react-vite": "9.0.12",
    "@storybook/test": "^8.6.14",
    "@tailwindcss/postcss": "^4.1.10",
    "@tailwindcss/vite": "^4.1.10",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/earcut": "^3.0.0",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^24.0.0",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@types/resolve": "^1.20.6",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "fast-check": "^4.1.1",
    "find-up": "^7.0.0",
    "glob": "^11.0.2",
    "jsdom": "^24.1.1",
    "resize-observer-polyfill": "^1.5.1",
    "resolve": "^1.22.10",
    "shx": "^0.3.4",
    "storybook": "9.0.12",
    "tailwindcss": "^4.1.10",
    "ts-morph": "^26.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^1.6.1",
    "vitest-canvas-mock": "^0.3.3",
    "vitest-fetch-mock": "^0.4.5"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.10.0"
}
