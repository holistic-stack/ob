{
  "name": "openscad-babylon",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "npx vitest run src/babylon-csg2 --reporter verbose",
    "test:run": "vitest run",
    "test:e2e": "npx vitest run src/babylon-csg2/e2e-tests --reporter=verbose",
    "validate": "npx tsx scripts/validate-complete-pipeline.ts",
    "validate:pipeline": "npx vitest run src/babylon-csg2/features/openscad/pipeline --reporter=verbose",
    "validate:visitor": "npx vitest run src/babylon-csg2/features/openscad/ast-visitor --reporter=verbose",
    "typecheck": "npx tsc --noEmit",
    "test:glob": "glob \"src/**/*.test.{ts,tsx}\"",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test:ct": "playwright test --config=playwright-ct.config.ts",
    "test:ct:ui": "playwright test --config=playwright-ct.config.ts --ui",
    "test:ct:headed": "playwright test --config=playwright-ct.config.ts --headed",
    "test:ct:debug": "playwright test --config=playwright-ct.config.ts --debug",
    "test:ct:update-snapshots": "playwright test --config=playwright-ct.config.ts --update-snapshots",
    "test:ct:hello": "playwright test src/shared/components/ui/ui-components.vspec.tsx --config=playwright-ct.config.ts --reporter=line",
    "test-ct": "playwright test -c playwright-ct.config.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@babylonjs/core": "^8.11.0",
    "@babylonjs/loaders": "^8.11.0",
    "@babylonjs/materials": "^8.11.0",
    "@holistic-stack/openscad-parser": "0.1.2",
    "@holistic-stack/tree-sitter-openscad": "0.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "find-up": "^7.0.0",
    "react": "19.0.0",
    "react-babylonjs": "latest",
    "react-dom": "19.0.0",
    "react-error-boundary": "^6.0.0",
    "web-tree-sitter": "0.25.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@playwright/experimental-ct-react": "^1.53.0",
    "@playwright/test": "^1.53.0",
    "@storybook/addon-a11y": "^9.0.12",
    "@storybook/addon-backgrounds": "^9.0.8",
    "@storybook/addon-docs": "9.0.12",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-onboarding": "9.0.12",
    "@storybook/addon-viewport": "^9.0.8",
    "@storybook/react-vite": "9.0.12",
    "@storybook/test": "^8.6.14",
    "@tailwindcss/vite": "^4.1.10",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^24.0.0",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@types/resolve": "^1.20.6",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/eslint-plugin": "^1.2.2",
    "eslint": "~9.8.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "9.0.12",
    "glob": "^11.0.2",
    "globals": "^16.2.0",
    "jsdom": "^24.1.1",
    "manifold-3d": "^3.1.1",
    "prettier": "^3.3.3",
    "resolve": "^1.22.10",
    "shx": "^0.3.4",
    "storybook": "9.0.12",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^1.6.0",
    "vitest-fetch-mock": "^0.4.5"
  },
  "keywords": [],
