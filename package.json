{
  "name": "openscad-r3f",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest run",
    "test:run": "vitest run",
    "validate": "npx tsx scripts/validate-complete-pipeline.ts",
    "validate:pipeline": "vitest run src/features/openscad-pipeline --reporter=verbose",
    "validate:r3f": "vitest run src/features/r3f-renderer --reporter=verbose",
    "typecheck": "npx tsc --noEmit",
    "test:glob": "glob \"src/**/*.test.{ts,tsx}\"",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "biome:check": "biome check --write .",
    "biome:lint": "biome lint .",
    "biome:format": "biome format --write .",
    "biome:fix": "biome check --write --unsafe .",
    "type-check": "tsc --noEmit",
    "test:ct": "playwright test --config=playwright-ct.config.ts",
    "test:ct:ui": "playwright test --config=playwright-ct.config.ts --ui",
    "test:ct:headed": "playwright test --config=playwright-ct.config.ts --headed",
    "test:ct:debug": "playwright test --config=playwright-ct.config.ts --debug",
    "test:ct:update-snapshots": "playwright test --config=playwright-ct.config.ts --update-snapshots",
    "test:ct:hello": "playwright test src/shared/components/ui/ui-components.vspec.tsx --config=playwright-ct.config.ts --reporter=line",
    "test-ct": "playwright test -c playwright-ct.config.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:all": "npx tsx scripts/test-runner.ts",
    "test:unit": "vitest run",
    "test:unit:memory-safe": "node --expose-gc scripts/run-tests-with-memory-management.js",
    "test:integration": "vitest run",
    "test:visual": "playwright test --config playwright-ct.config.ts",
    "test:pipeline": "tsx scripts/test-runner.ts --suite=integration",
    "test:ci": "tsx scripts/test-runner.ts --ci --skip-optional",
    "test:watch": "vitest",
    "test:watch:integration": "vitest",
    "test:coverage:all": "tsx scripts/test-runner.ts --coverage",
    "test:ast-service": "vitest run src/features/ui-components/editor/code-editor/openscad-ast-service.test.ts",
    "test:error-boundary": "vitest run src/features/ui-components/shared/error-boundary/openscad-error-boundary.test.tsx",
    "test:r3f-visual": "playwright test src/features/r3f-renderer/components/r3f-renderer/r3f-renderer.vspec.tsx --config playwright-ct.config.ts --update-snapshots",
    "validate:shared-utilities": "node scripts/validate-shared-utilities.js",
    "validate:types": "tsc --noEmit --strict",
    "validate:quality": "npm run lint && npm run validate:types && npm run validate:shared-utilities",
    "validate:all": "npm run validate:quality && npm run test:unit && npm run test:coverage",
    "fix:auto": "tsx scripts/ollama-lint-fixer.ts",
    "fix:auto:codegemma": "tsx scripts/ollama-lint-fixer.ts --model=codegemma:7b",
    "fix:auto:phi3": "tsx scripts/ollama-lint-fixer.ts --model=phi3:3.8b"
  },
  "dependencies": {
    "@monaco-editor/react": "4.7.0",
    "@react-three/drei": "^10.3.0",
    "@react-three/fiber": "^9.1.2",
